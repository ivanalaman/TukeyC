S3method(TukeyC, nest.aovlist)
S3method(TukeyC, nest.lmerMod)
S3method(summary, TukeyC)
S3method(print, TukeyC)
S3method(print,xtable)
S3method(xtable,TukeyC)
S3method(plot, TukeyC)
S3method(boxplot, TukeyC)
